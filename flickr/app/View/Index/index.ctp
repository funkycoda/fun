<div id="searchwrap">
	<h3>Welcome to Flearch, yet another flickr search tool.</h3>
	<form id="frmSearch">
		<label for="q">Enter search terms</label>
		<input id="q" type="text" placeholder="Enter search terms" autocomplete="off" value="Sydney Central Station" />
		<input id="btnsearch" class="button green" type="button" value="Search" />
	</form>
</div>
<div id="resultswrap">
	<p>Found <span id="pcount"></span> results</p>
	<div id="photos"></div>
	<ol id="paginate"></ol>
</div>
